<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="363.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.7" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.uv.fei.gui.controller.AdminReportController">
   <children>
      <Button fx:id="buttonGenerateReport" disable="true" layoutX="232.0" layoutY="13.0" mnemonicParsing="false" onAction="#generateReportOnAction" style="-fx-background-color: #f1cb00;" text="Descargar reporte">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Cambria Bold" size="14.0" />
         </font></Button>
      <ComboBox fx:id="comboBoxNrc" disable="true" layoutX="431.0" layoutY="14.0" onAction="#setNrcOnImage" prefWidth="150.0" promptText="NRC" />
      <ComboBox fx:id="comboBoxPeriod" disable="true" layoutX="20.0" layoutY="14.0" onAction="#setPeriodOnImage" prefWidth="150.0" promptText="Periodo" />
      <ScrollPane fx:id="scrollPane" layoutX="20.0" layoutY="60.0" prefHeight="273.0" prefWidth="562.0">
         <content>
            <ImageView fx:id="imageView" fitHeight="460.0" fitWidth="664.0" pickOnBounds="true" preserveRatio="true" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
